{
    "swagger": "2.0",
    "info": {
        "title": "bank-transactions API",
        "contact": {},
        "version": "1.0"
    },
    "paths": {
        "/api/v1/download_remote_transactions/": {
            "post": {
                "description": "The request runs fake ~40 seconds. If url=‚Äù or download in progress, returns the status of the download.\n\nThe amount of memory consumed depends on the transactionCount and workerCount in the internal\\services\\worker_pool.go file.",
                "tags": [
                    "Services"
                ],
                "summary": "Request to download remote transactions",
                "operationId": "request-download-remote-transactions",
                "parameters": [
                    {
                        "description": " ",
                        "name": "input",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/domain.UrlInput"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/domain.StatusResponse"
                        }
                    },
                    "204": {
                        "description": "No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/filtered/csv/": {
            "post": {
                "tags": [
                    "Services"
                ],
                "summary": "Request filtered csv file",
                "operationId": "get-filtered-csv",
                "parameters": [
                    {
                        "description": " ",
                        "name": "input",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/domain.FilterSearchInput"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "204": {
                        "description": "No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/filtered/json/": {
            "post": {
                "tags": [
                    "Services"
                ],
                "summary": "Request filtered json",
                "operationId": "get-filtered-json",
                "parameters": [
                    {
                        "description": " ",
                        "name": "input",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/domain.FilterSearchInput"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/domain.Transaction"
                            }
                        }
                    },
                    "204": {
                        "description": "No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/get_csv_mock_remote_service/": {
            "get": {
                "tags": [
                    "Mock remote service"
                ],
                "summary": "Test service: Gives a CSV file with initial transactions",
                "operationId": "getSourceFileCSV_as_MockRemoteService-csv",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "From transaction, example: 1",
                        "name": "from",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "To transaction, example: 5 (TO must be greater than FROM, if both are present at the same time)",
                        "name": "to",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "204": {
                        "description": "No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "domain.ErrorResponse": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string"
                }
            }
        },
        "domain.FilterSearchInput": {
            "type": "object",
            "properties": {
                "payment_narrative": {
                    "type": "string",
                    "example": "contract for the provision of services A11/27123"
                },
                "payment_type": {
                    "type": "string",
                    "enum": [
                        "cash",
                        "card"
                    ],
                    "example": "cash"
                },
                "period": {
                    "$ref": "#/definitions/domain.Period"
                },
                "status": {
                    "type": "string",
                    "enum": [
                        "accepted",
                        "declined"
                    ],
                    "example": "accepted"
                },
                "terminal_id": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    },
                    "example": [
                        3521,
                        3522,
                        3523,
                        3524,
                        3525,
                        3526,
                        3527,
                        3528,
                        3529
                    ]
                },
                "transaction_id": {
                    "type": "integer",
                    "example": 18
                }
            }
        },
        "domain.Period": {
            "type": "object",
            "required": [
                "from",
                "to"
            ],
            "properties": {
                "from": {
                    "description": "example: from 2023-08-12, to 2023-09-01 must return all transactions for the specified period",
                    "type": "string",
                    "example": "2023-08-23T11:56:00.000Z"
                },
                "to": {
                    "type": "string",
                    "example": "2023-08-24T00:00:00.000Z"
                }
            }
        },
        "domain.StatusResponse": {
            "type": "object",
            "properties": {
                "last_download_status": {
                    "type": "string",
                    "enum": [
                        "unknown",
                        "in progress",
                        "skip request: download in progress",
                        "successfully",
                        "error"
                    ],
                    "example": "unknown"
                }
            }
        },
        "domain.Transaction": {
            "type": "object",
            "properties": {
                "amount_original": {
                    "type": "number",
                    "example": 1.23
                },
                "amount_total": {
                    "type": "number",
                    "example": 1.23
                },
                "commission_client": {
                    "type": "number",
                    "example": 1.23
                },
                "commission_provider": {
                    "type": "number",
                    "example": 1.23
                },
                "commission_ps": {
                    "type": "number",
                    "example": 1.23
                },
                "date_input": {
                    "type": "string"
                },
                "date_post": {
                    "type": "string"
                },
                "partner_object_id": {
                    "type": "integer"
                },
                "payee_bnank_account": {
                    "type": "string"
                },
                "payee_bnank_mfo": {
                    "type": "integer"
                },
                "payee_id": {
                    "type": "integer"
                },
                "payee_name": {
                    "type": "string"
                },
                "payment_narrative": {
                    "type": "string"
                },
                "payment_number": {
                    "type": "string"
                },
                "payment_type": {
                    "type": "string"
                },
                "request_id": {
                    "type": "integer"
                },
                "service": {
                    "type": "string"
                },
                "service_id": {
                    "type": "integer"
                },
                "status": {
                    "type": "string"
                },
                "terminal_id": {
                    "type": "integer"
                },
                "transaction_id": {
                    "type": "integer"
                }
            }
        },
        "domain.UrlInput": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "example": "http://localhost:8080/api/v1/get_csv_mock_remote_service"
                }
            }
        }
    }
}